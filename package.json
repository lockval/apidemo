{
  "name": "apidemo.lockval",
  "version": "1.0.0",
  "scripts": {
    "buildAndPreview": "run-s lint build preview",
    "build": "run-s serverGen serverUpload clientGen check-build",
    "check-build": "run-p type-check build-only",
    "build-only": "vite build",
    "preview": "vite preview",

    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.jsx,.cjs,.mjs,.js,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",

    "clientYAML2JSON": "yaml2json client.yaml > ./src/assets/client.json",
    "clientMakePlayer": "./makeplayer.js $lockvalGwAddrs",
    "clientGen": "run-s clientYAML2JSON clientMakePlayer",
    "serverUpload": "curl --insecure -X POST --data-binary @public/server.js `./getUpdatefileURL.js $lockvalUpdatefileDomain $lockvalUpdatefilePassword`",
    "serverGen": "tsc --project ./public/server/javascript/tsconfig.json"

  },
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "vue": "^3.2.41",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "mustache": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vue-tsc": "^1.0.8",
    "yamljs": "^0.3.0"
  }
}
